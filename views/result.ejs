<html>
<head>
  <title><%=product%></title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="icon" href="/images/favicon.png" type="image/x-icon">
  <link type="text/css" rel="stylesheet" href="/css/bootstrap.min.css" />
  <script src="https://use.edgefonts.net/lobster-two.js"></script>
  <style>
     p.headingFont{
        font-family: lobster-two, cursive;
        font-size: 40px;
   }
  </style>
</head>
<body>
  <!--  Top Fixed Navigation bar  -->
  <div class="navbar navbar-default navbar-fixed-top">
    <div class="container">
       <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="icon-bar" ></span>
            <span class="icon-bar" ></span>
            <span class="icon-bar" ></span>
         </button>
         <a href="/" class="navbar-brand">Home!</a>
      </div>
      <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li><a href="/">query1</a></li>
            <li><a href="/">query2</a></li>
            <li><a href="/">query3</a></li>
            <li><a href="/">query4</a></li>
          </ul>
      </div>
   </div>
  </div>
    <!--  Complete Top Fixed Navigation bar -->
  <br/><br/><br/>
  

  <div class="container">
       <div class="row">
          <div class="col-md-12">
                  <p class="" align="left">
                     <%=product%>  <br> <%=email%> <br>    <%=version%> <br>
					 
					
                  </p>
          </div>
        </div>
  </div>
   <div class="container">
          <div class="row">
		 
              
              
              <div class="col-md-6">

                
                 <table class="table table-striped">
				 <thead>
            <tr>
              <th>Bug ID</th>
            </tr>
			
          </thead>


					<% for(var i=0; i < result.length; i++) { %>
						<tr class="active">
							<td class="success"><%= result[i].bug_id %></td>
							<td><a href='/getChilds?bug_id=<%=result[i].bug_id%>'>
                <span class="glyphicon glyphicon-asterisk"></span>
              </a></td>
     
						</tr>
						<% } %>
				</table>
				
				
				
				<table class="table table-striped">
				 <thead>
            <tr>
			<%if (base){%>
              <th>child for Base: <%=base%></th>
			  <%}%>
            </tr>
          </thead>

					<%if(child){%>
					<% for(var i=0; i < child.length; i++) { %>
				
						<tr class="active">
							<td class="success"><%= child[i].child %></td>
						</tr>
						
						<% } } %>
						
						
				</table>
				
				
				
				
				
				
				
				
		
              </div>
              
              
          </div>
  </div>

  <br/>
    

<script src="/js/jquery-1.11.3.js"></script>
<script src="/js/bootstrap.min.js"></script>
</body>
</html>
